{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "app.js", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "api/youtube-viewers.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" }
  ],
  "functions": {
    "api/youtube-viewers.js": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}
